#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules/jest/bin/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules/jest/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/jest@27.5.1/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/jest.js" "$@"
else
  exec node  "$basedir/../../bin/jest.js" "$@"
fi
