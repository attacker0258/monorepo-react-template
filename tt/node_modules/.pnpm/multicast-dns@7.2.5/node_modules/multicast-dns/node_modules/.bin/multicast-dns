#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/workspaces/monorepo-react-template/tt/node_modules/.pnpm/multicast-dns@7.2.5/node_modules/multicast-dns/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/multicast-dns@7.2.5/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/workspaces/monorepo-react-template/tt/node_modules/.pnpm/multicast-dns@7.2.5/node_modules/multicast-dns/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/multicast-dns@7.2.5/node_modules:/workspaces/monorepo-react-template/tt/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
